<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/flMainFrame"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <com.gradtech.mapframev10.core.maps.GMapView
            android:id="@+id/mapView"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </com.gradtech.mapframev10.core.maps.GMapView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <Button
                android:id="@+id/btn_auto_create"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="自动生成"></Button>
            <Button
                android:id="@+id/btn_custum_create"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="原生方式生成"></Button>
            <Button
                android:id="@+id/btn_auto_activity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="用户自动调节生成"></Button>
        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>