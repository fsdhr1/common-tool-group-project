<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SketchTestActivity">
    <FrameLayout
        android:id="@+id/flMainFrame"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <Button
                android:id="@+id/ks"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="开始打点线" />

            <Button
                android:id="@+id/ksdhx1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="开始点划线" />

            <Button
                android:id="@+id/ksdhm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="开始点划面" />

            <Button
                android:id="@+id/jd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="加点" />

            <Button
                android:id="@+id/undo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="回撤" />

            <Button
                android:id="@+id/redo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="前撤" />

            <Button
                android:id="@+id/addgeo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="添加图形" />

            <Button
                android:id="@+id/qk"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="清空" />

            <Button
                android:id="@+id/js"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="结束" />

            <Button
                android:id="@+id/btn_add2gc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="添加到GC" />
        </LinearLayout>

    </FrameLayout>
    <!--<com.gradtech.mapbox.mapframev10.core.maps.GMapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:mapbox_cameraTargetLat="59.31"
        mapbox:mapbox_cameraTargetLng="18.06"
        mapbox:mapbox_cameraZoom="9.0"
        />-->

</androidx.constraintlayout.widget.ConstraintLayout>